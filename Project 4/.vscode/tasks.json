{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "g++ build project 4",
      "command": "/usr/bin/g++",
      "args": [
        "-g",
        "parser.cc",
        "compiler.cc",
        "inputbuf.cc",
        "ir_debug.cc",
        "lexer.cc",
        "-o",
        "a.out"
      ],
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "shell",
      "label": "g++ build active file",
      "command": "/usr/bin/g++",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "/usr/bin"
      }
    }
  ]
}